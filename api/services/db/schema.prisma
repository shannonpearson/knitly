generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id         String    @id @db.Uuid
  firstname  String?
  lastname   String?
  email      String?
  created_on DateTime? @db.Timestamp(6)
  updated_on DateTime? @db.Timestamp(6)
  last_login DateTime? @db.Timestamp(6)
}
